# Benchmark 1.0

import tensorflow as tf
import time from datetime 
import timedelta

math_func_1param = 
    [tf.abs,tf.negative,tf.sign,tf.square,tf.round,tf.sqrt,tf.rsqrt,tf.exp,tf.log,tf.ceil,tf.floor,tf.cos,tf.sin,tf.tan,tf.acos,tf.asin,tf.atan,tf.lgamma,tf.digamma,tf.erf,tf.erfc]
math_func_2param = 
    [tf.add,tf.subtract,tf.multiply,tf.div,tf.truediv,tf.floordiv,tf.mod,tf.pow,tf.maximum,tf.minimum,tf.squared_difference,tf.igamma,tf.igammac,tf.zeta,tf.polygamma]

a = tf.constant(2.0)

start_time = time.time()
with tf.Session() as sess:    
  for i in range(0, 10):        
    for i in range(0,len(math_func_1param)):            
      f = math_func_1param[i]            
      result = sess.run(f(a))            
      #print ("result:" + str(result) + " , " + str(f) )
    for i in range(0,len(math_func_2param)):            
      f = math_func_2param[i]            
      result = sess.run(f(a,a))            
      #print ("result:" + str(result) + " , " + str(f) )
      
end_time = time.time()
time_dif = end_time - start_time

# 사용 시간 출력
print("Time usage of (math_func): " + str(timedelta(seconds=int(round(time_dif)))))
